{
  "manifest_version": 3,
  "name": "PICYBOO Browser Stub Extension (MV3)",
  "version": "0.2.0",
  "description": "Demonstration extension for curating audit-ready inspiration captures.",
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "icon48.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "contextMenus",
    "tabs"
  ]
}
